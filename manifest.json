{
  "manifest_version": 3,
  "version": "0.2",
  "name": "Patreon Course Viewer",
  "description": "Use patreon as if it is a course platform",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": {
      "16": "public/16-grey.png",
      "32": "public/32-grey.png",
      "48": "public/48-grey.png",
      "128": "public/128-grey.png"
    }
  },
  "icons": {
    "16": "public/16.png",
    "32": "public/32.png",
    "48": "public/48.png",
    "128": "public/128.png"
  }
}
